import java.io.*;
import java.util.*;

public class MyClass {
	
    

	public static void main(String[] args) 
       {
		int first;
	      Scanner scan = new Scanner(System.in);
	      int n=scan.nextInt();
	      int arr[]=new int[n];
	      
	      for(int i=0;i<n;i++)
	         arr[i]=scan.nextInt();
	      
	      first=arr[0];
	   
	      for(int i=0;i<n-1;i++)
	    	  arr[i]=arr[i+1];
	      
	      arr[n-1]=first;
	      
	      for(int i=0;i<n;i++)
	    	  System.out.print(arr[i]+" ");
	    
       }
}
